@incollection{stanfordUnderdeterminationScientificTheory2021,
  title = {Underdetermination of {{Scientific Theory}}},
  booktitle = {The {{Stanford Encyclopedia}} of {{Philosophy}}},
  author = {Stanford, Kyle},
